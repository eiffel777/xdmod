{
    "table_definition": {
        "$ref": "${table_definition_dir}/common/staging/union-user-pi.json#/table_definition"
    },
    "source_query": {
        "records": {
            "union_user_pi_name": "DISTINCT IF(raw.user_name IS NULL, 'Unknown', raw.user_name)"
        },
        "joins": [
            {
                "name": "openstack_raw_event",
                "schema": "${SOURCE_SCHEMA}",
                "alias": "raw"
            }
        ]
    }
}
